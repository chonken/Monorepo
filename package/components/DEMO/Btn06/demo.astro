---
import Btn06 from './index.astro'

export const id = 'btn_type06'
export const props = {
  href: { type: ['string'], require: false, default: '#', demo: '#' },
  class: { type: ['string'], require: false, default: '' },
  color: { type: ['string'], require: false, default: 'inherit' },
  backgroundColor: { type: ['string'], require: false, default: '#eef1f6', demo: '#8888' },
  borderColor: { type: ['string'], require: false, default: '#00b39b'},
  hoverBorderColor: { type: ['string'], require: false, default: '#2b4346', demo: '#888' },
  blank: { type: ['boolean'], require: false, default: false },
}
export const slots = ['default', 'icon']
export const keywords = []
export const categrory = []
const data = Astro.props.data['button']?.[0] ?? {}
---

<Btn06 href={props.href.demo} backgroundColor={props.backgroundColor.demo} hoverBorderColor={props.hoverBorderColor.demo}>
  {data.text}
</Btn06>
