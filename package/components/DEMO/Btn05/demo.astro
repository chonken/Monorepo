---
import Btn05 from './index.astro'

export const id = 'btn_type05'
export const props = {
  href: { type: ['string'], require: false, default: '#', demo: '#' },
  class: { type: ['string'], require: false, default: '' },
  color: { type: ['string'], require: false, default: 'inherit' },
  backgroundColor: { type: ['string'], require: false, default: '#333', demo: '#888' },
  borderColor: { type: ['string'], require: false, default: '#333', demo: '#888' },
  blank: { type: ['boolean'], require: false, default: false },
}
export const slots = ['default', 'icon']
export const keywords = []
export const categrory = []
const data = Astro.props.data['button']?.[0] ?? {}
---

<Btn05 href={props.href.demo} backgroundColor={props.backgroundColor.demo} borderColor={props.borderColor.demo}>
  {data.text}
</Btn05>
